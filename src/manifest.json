{
  "name": "Keep open",
  "description": "Prevents closing of Google Chrome with the last tab.",
  "version": "0.4.0",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ]
}
